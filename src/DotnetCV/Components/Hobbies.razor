<section id="hobbies" >
    <h4>Hobbies</h4>
    <div class="row">
        @if (_isLoaded)
        {
            foreach (var hobby in _hobbies)
            {
                <div class="col hobby">
                    <p>
                        @hobby
                    </p>
                </div>
            }
        }
    </div>
</section>