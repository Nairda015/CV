<section id="languages">
    <h5>Languages</h5>
    @if (_isLoaded)
        {
            @foreach (var (name, level) in _languages)
            {
                <div class="row skill">
                    <div class="col-lg-7">@name</div>
                    <div class="col-lg-5">@level</div>
                </div>
            }
        }
</section>