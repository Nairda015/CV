<div id="workExperience">
    <h3>WorkExperience</h3>
    @foreach (var job in _jobs)
    {
        <div>
            <div class="jobHeading">
                <h4>@job.Position</h4>
                <h5>@job.Company</h5>
                <div class="jobDate">
                    <p>@job.GetTimeStamp()</p>
                </div>
            </div>
            @if (!string.IsNullOrEmpty(job.Description))
            {
                <div class="jobDescription">
                    <p>@job.Description</p>
                </div>
            }
        </div>
    }
</div>